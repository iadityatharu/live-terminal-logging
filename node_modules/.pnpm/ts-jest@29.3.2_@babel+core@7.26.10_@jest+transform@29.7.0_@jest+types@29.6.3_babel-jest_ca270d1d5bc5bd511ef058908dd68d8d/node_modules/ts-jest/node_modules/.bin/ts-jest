else
  export NODE_PATH="/Users/adityatharu/Desktop/live terminal logger/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_ca270d1d5bc5bd511ef058908dd68d8d/node_modules/ts-jest/node_modules:/Users/adityatharu/Desktop/live terminal logger/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_ca270d1d5bc5bd511ef058908dd68d8d/node_modules:/Users/adityatharu/Desktop/live terminal logger/node_modules/.pnpm/node_modules:$NODE_PATH"
